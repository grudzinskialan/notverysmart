{
  "manifest_version": 3,
  "name": "emoji inserter",
  "version": "0.1",
  "description": "make the text you're reading dumbass-friendly by inserting emojis randomly!",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "emoji inserter"
  }
}
